{
  "name": "aline-test",
  "version": "1.1.0",
  "description": "Aline test task",
  "scripts": {
    "setup": "npm install",
    "start:webpack": "cross-env NODE_ENV=development webpack-dev-server --progress",
    "start:api": "cross-env NODE_ENV=development ./node_modules/.bin/nodemon --watch ./server ./server/run.js -- --verbose --es_staging",
    "start:api:prod": "cross-env NODE_ENV=production node ./server/run.js --es_staging",
    "build": "cross-env NODE_ENV=production webpack --progress --optimize-minimize",
    "prestart": "npm run build",
    "start": "npm run start:api:prod",
    "test": "cross-env NODE_ENV=test node_modules/.bin/karma start --single-run"
  },
